{
  "name": "curaq-clipper",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "wxt prepare",
    "dev": "wxt",
    "build": "wxt build && wxt zip",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "format": "biome check --write",
    "update": "pnpm update --latest --interactive"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "typescript": "5.9.3",
    "wxt": "0.20.13"
  },
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": ">=24",
        "onFail": "download"
      }
    ]
  }
}
